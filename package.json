    "frames", 
    "sections", 
    "links", 
    "copy", 
    "multilingual", 
    "productivity", 
    "design-tools",
    "workflow"
  ],
  "author": "mautre",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mautre/not-a-linker"
  },
  "homepage": "https://github.com/mautre/not-a-linker#readme",
  "bugs": {
